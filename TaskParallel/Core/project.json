{
  "title": "Provides a backport of Task for .NET 3.5 Compact Edition",
  "version": "3.0.0-*",
  "language": "en-US",
  "authors": [ "Fabricio Godoy" ],
  "packOptions": {
    "owners": [ "skarllot", "fgodoy-verion" ],
